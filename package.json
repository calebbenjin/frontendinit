{
  "name": "frontendinit",
  "version": "0.1.0",
  "description": "The Frontend Engineer Growth Community.",
  "private": true,
  "author": "Caleb Benjamin <hey@calebben.dev> (@calebbenjin)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "mdx",
    "eslint",
    "tailwindcss",
    "styled-component",
    "Job Openings",
    "Interview Preparation",
    "Newsletter that will 10x Your Frontend Engineering Career",
    "Access to best practices",
    "Access To Free Learning Resources",
    "Frontend Developer Roadmap",
    "free resources",
    "web developement free learning resources",
    "typescript"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "next": "13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "react-icons": "^4.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "4.8.4",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3"
  }
}